save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0},"int":{"value":8,"proficient":0,"bonuses":{"check":"","save":""},"min":3,"mod":-1,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0},"wis":{"value":14,"proficient":0,"bonuses":{"check":"","save":""},"min":3,"mod":2,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0},"cha":{"value":14,"proficient":0,"bonuses":{"check":"","save":""},"min":3,"mod":2,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0}},"attributes":{"ac":{"flat":15,"calc":"natural","formula":"","min":12},"hp":{"value":27,"min":0,"max":27,"temp":null,"tempmax":null,"formula":"6d6+6"},"init":{"value":0,"bonus":0,"mod":0,"total":0,"prof":0},"movement":{"burrow":0,"climb":0,"fly":0,"swim":0,"walk":30,"units":"ft","hover":false},"attunement":{"max":3},"senses":{"darkvision":0,"blindsight":0,"tremorsense":0,"truesight":0,"units":"ft","special":""},"spellcasting":"","speed":{"walk":25},"prof":0,"spelldc":10,"spellLevel":0,"death":{"success":0,"failure":0}},"details":{"biography":{"value":"<p>Каррионетки возникают из невинных намерений. Искренние желания вдохнуть жизнь в любимую игрушку и на время создатель может почувствовать себя благословленным своим новым спутником. Но карионетки не довольствуются тем, чтобы жить как игрушки, и стремятся вырваться из рамок своих крошечных тел.</p><p>У каждой каррионетки есть серебряная игла, которая прикалывает душу к телу. Выдавая себя за простые игрушки или скрывая свои желания, она как падальщик приближается к ничего не подозревающей жертве. Затем она использует свою иглу, чтобы обменятся душами с жертвой, похищая тело жертвы, в то же время заключая душу жертвы в свою кукольную форму. Затем каррионетка заключает в тюрьму свое старое тело, скрывая одушевленную куклу, пока она исследует мир в ее украденном обличье - часто в облике того самого человека, который хотел, чтобы каррионетка возникла.</p><p>Каррионетки могут выглядеть как обычная игрушка или произведение искусства. В то время как марионетки и фарфоровые куклы являются наиболее распространенными, всевозможные смертоносные чучела животных, ползучие домкраты из коробок, кровожадные куклы, кровожадные балерины из шкатулки для драгоценностей и т. д. могут быть карионетками. Эти злобные игрушки являются искусными обманщиками.Некоторые из них существуют в течение нескольких поколений и часто являются причиной детских страхов.</p><p>Среди самых известных ужасов - падальщик Малиньо, Темный лорд из владений Одайре.</p>","public":""},"alignment":"без мировоззрения","race":null,"type":{"value":"construct","subtype":"","swarm":"","custom":""},"environment":"","cr":1,"spellLevel":0,"xp":{"value":200},"source":"VRGR","gender":"","age":"","height":"","weight":"","eyes":"","skin":"","hair":"","notes1name":"","notes2name":"","notes3name":"","notes4name":"","class":null},"traits":{"size":"sm","di":{"value":[],"bypasses":[],"custom":""},"dr":{"value":["poison","psychic"],"bypasses":[],"custom":""},"dv":{"value":[],"bypasses":[],"custom":""},"ci":{"value":["charmed","frightened","poisoned"],"custom":""},"languages":{"value":[],"custom":"понимает языки своего создателя"},"senses":"","skills":{"acr":{"value":0,"ability":"dex","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"arc":{"value":0,"ability":"int","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"ath":{"value":0,"ability":"str","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"dec":{"value":0,"ability":"cha","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"his":{"value":0,"ability":"int","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"ins":{"value":0,"ability":"wis","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"itm":{"value":0,"ability":"cha","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"inv":{"value":0,"ability":"int","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"med":{"value":0,"ability":"wis","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"nat":{"value":0,"ability":"int","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"prc":{"value":0,"ability"