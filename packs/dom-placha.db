":"","save":""},"min":3,"mod":3,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0},"dex":{"value":13,"proficient":0,"bonuses":{"check":"","save":""},"min":3,"mod":1,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0},"con":{"value":14,"proficient":0,"bonuses":{"check":"","save":""},"min":3,"mod":2,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0},"int":{"value":10,"proficient":0,"bonuses":{"check":"","save":""},"min":3,"mod":0,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0},"wis":{"value":11,"proficient":0,"bonuses":{"check":"","save":""},"min":3,"mod":0,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0},"cha":{"value":10,"proficient":0,"bonuses":{"check":"","save":""},"min":3,"mod":0,"save":0,"prof":0,"saveBonus":0,"checkBonus":0,"dc":0}},"attributes":{"ac":{"flat":17,"calc":"default","formula":"","min":11},"hp":{"value":58,"min":0,"max":58,"temp":null,"tempmax":null,"formula":"9d8+18"},"init":{"value":0,"bonus":0,"mod":0,"total":0,"prof":0},"movement":{"burrow":0,"climb":0,"fly":0,"swim":0,"walk":30,"units":"ft","hover":false},"attunement":{"max":3},"senses":{"darkvision":0,"blindsight":0,"tremorsense":0,"truesight":0,"units":"ft","special":""},"spellcasting":"","speed":{"walk":30},"prof":0,"spelldc":10,"spellLevel":0},"details":{"biography":{"value":"<p>Дочери мэра Мордентшира Элис Уэзермей, близнецы Дженнифер и Лаура росли неразлучными хулиганами. Незадолго до их шестнадцатилетия дядя близнецов, известный охотник на монстров Джордж Уэзермей, вернулся в Мордент со своей невестой Натальей Вхоришковой. Близнецы поняли, что Наталья манипулировала их любимым дядей, в конце концов разоблачив ее как оборотня, который использовал Джорджа, чтобы преследовать одного из других известных жителей Мордента, Рудольфа ван Рихтена. После того, как близнецы спасли ван Рихтена и их дядю от оборотня, и Джордж, и ван Рихтен поощряли их исследовательские инстинкты, обучая их охотникам на монстров.</p><p>С тех пор Лори и Дженнифер стали неутомимыми авантюристами. Лори, по примеру своего дяди, обучалась боевым приемам борьбы с нежитью. Она редко путешествует без своих компаньонов Джоан и Тирран, взрослых щенков гончих ее дяди и волшебного меча ее дяди, Паутинки.</p><p></p><h1>Черты Лори</h1><ul><li><p>Идеал. «Всему есть решение. Я просто должен воспользоваться твоим шансом, когда он появится».</p></li><li><p>Связь. «Дела, а не слова, сохранят уважаемое место моей семьи в обществе».</p></li><li><p>Недостаток. «Я никогда не буду таким умным, как мои родственники, поэтому мне придется быть сильнее».</p></li></ul>","public":""},"alignment":"любое мировоззрение","race":null,"type":{"value":"humanoid","subtype":"","swarm":"","custom":""},"environment":"полярная тундра, город, побережье, лес, равнина/луг, холмы, горы, подземье","cr":3,"spellLevel":0,"xp":{"value":700},"source":"MM","gender":"","age":"","height":"","weight":"","eyes":"","skin":"","hair":"","notes1name":"","notes2name":"","notes3name":"","notes4name":"","class":null},"traits":{"size":"med","di":{"value":[],"bypasses":[],"custom":""},"dr":{"value":[],"bypasses":[],"custom":""},"dv":{"value":[],"bypasses":[],"custom":""},"ci":{"value":[],"custom":""},"languages":{"value":["common"],"custom":"один любой язык (обычно Общий)"},"senses":"","skills":{"acr":{"value":0,"ability":"dex","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"arc":{"value":0,"ability":"int","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"ath":{"value":1,"ability":"str","bonus":0,"mod":5,"passive":0,"prof":0,"total":0},"dec":{"value":0,"ability":"cha","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"his":{"value":0,"ability":"int","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"ins":{"value":0,"ability":"wis","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"itm":{"value":0,"ability":"cha","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"inv":{"value":0,"ability":"int","bonus":0,"mod":0,"passive":0,"prof":0,"total":0},"med":{"value":0,"ability":"wis","bonus":0,"mod":0,"passive":0,"prof":0,